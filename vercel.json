{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/login", "dest": "/routes/login" },
      { "src": "/register", "dest": "/routes/register" },
      { "src": "/contact", "dest": "/routes/contact" },
      { "src": "/(.*)", "dest": "/index.js" }
    ]
  }
  